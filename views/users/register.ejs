<%- include('../partials/header.ejs') %>
<%- include('../partials/navbar.ejs') %>
<div class="breadcrumb-area contact-breadcrumb bg-img bg-overlay jarallax"
    style="background-image: url(/img/bg-img/18.jpg);">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="breadcrumb-content text-center mt-100">
                    <h2 class="page-title">Register</h2>
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb justify-content-center">
                            <li class="breadcrumb-item"><a href="/">Home</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Register</li>
                        </ol>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</div>
<section class="google-maps-contact-info" style="margin-bottom: 5rem;">
    <div class="container-fluid">
        <div class="google-maps-contact-content">
            <div class="row">
                <div class="col-12">
                    <div class="section-heading text-center wow fadeInUp" data-wow-delay="100ms">
                        <h6>Account Management</h6>
                        <h2>Sign Up</h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-12">
                    <div class="roberto-contact-form">
                        <form action="/register" method="post">
                            <div class="row">
                                <div class="col-4" style="margin: auto;">
                                    <div class="row">
                                        <div class="col-12 col-lg-12 wow fadeInUp" data-wow-delay="100ms">
                                            <input type="email" name="username" class="form-control mb-30"
                                                placeholder="name@example.com" required>
                                        </div>
                                        <div class="col-12 col-lg-12 wow fadeInUp" data-wow-delay="100ms">
                                            <input type="password" name="password" class="form-control mb-30"
                                                placeholder="Your Password" required>
                                        </div>
                                        <div class="col-12 col-lg-12 wow fadeInUp" data-wow-delay="100ms">
                                            <input type="text" name="image" class="form-control mb-30"
                                                placeholder="Avatar URL" required>
                                        </div>
                                        <div class="col-12 text-center wow fadeInUp" data-wow-delay="100ms">
                                            <button style="width: 100%;" type="submit"
                                                class="btn roberto-btn mt-15">Sign Up</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
